{
  "name": "spaghettify",
  "version": "1.0.0-rc1",
  "description": "JavaScript module that replaces regular navigation by an AJAX-driven experience.",
  "main": "dist/index.js",
  "repository": "https://github.com/deeleman/spaghettify",
  "author": "Pablo Deeleman <deeleman@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {

  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.26.0",
    "jest": "^26.6.3",
    "lite-server": "^2.6.1",
    "parcel": "^2.0.0-beta.2",
    "typescript": "^4.2.4"
  },
  "alias": {
    "spaghettify": "./src"
  }
}
